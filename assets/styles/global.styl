body{
    font: 14px/16px 'Open Sans', Arial, sans-serif;
    min-width: 320px;
}
h1{
    font-size: 32px;
    line-height: 40px;
}
h2{
    font-size: 20px;
    line-height: 26px;
}
h3{
    font-size: 16px;
    line-height: 20px;
}
p{
    margin: 0 0 10px;
}
#wrapper{
    max-width: 1003px;
    min-width: 400px;
    margin: 0 auto;
    padding: 20px 0;

    clearfix();
}

.page-holder{
    clearfix();
}
